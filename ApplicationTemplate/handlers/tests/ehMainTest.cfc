<!-----------------------------------------------------------------------Author 	 :	Your NameDate     :	September 25, 2005Description :	Unit test for the ehMain Handler.-----------------------------------------------------------------------><cfcomponent name="ehMainTest" extends="org.cfcunit.framework.TestCase" output="false">		<cffunction name="setUp" returntype="void" access="private"> 		<!--- Setup ColdBox Mappings --->		<cfset variables.AppMapping = "/applications/coldbox/ApplicationTemplate/">		<cfset variables.HandlerMapping = ExpandPath(AppMapping & "handlers")>		<cfset variables.ConfigMapping = ExpandPath(AppMapping & "config/config.xml.cfm")>		<!--- Initalize ColdBox --->		<cfset variables.controller = CreateObject("component", "coldbox.system.controller").init()>		<!--- Load Config --->		<cfset variables.controller.getService("loader").configLoader(variables.ConfigMapping)>		<!--- Set App Mapping --->		<cfset variables.controller.setSetting("AppMapping", variables.AppMapping)>		<cfset variables.controller.setSetting("HandlersPath",variables.HandlerMapping)>		<!--- Finish Registration --->		<cfset variables.controller.getService("loader").registerHandlers()>	</cffunction>		<cffunction name="testonAppInit" access="public" returntype="void" output="false">			</cffunction>	<cffunction name="testonRequestStart" access="public" returntype="void" output="false">			</cffunction>	<cffunction name="testonRequestEnd" access="public" returntype="void" output="false">			</cffunction>	<cffunction name="testonException" access="public" returntype="void" output="false">			</cffunction></cfcomponent>